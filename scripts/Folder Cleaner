import os
import time

folder_path = "path/to/folder"
days = 30
now = time.time()

for file in os.listdir(folder_path):
    path = os.path.join(folder_path, file)
    if os.path.isfile(path):
        if os.stat(path).st_mtime < now - days * 86400:
            os.remove(path)
            print(f"Deleted: {file}")
